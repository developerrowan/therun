{
  "name": "therungg",
  "version": "1.0.0",
  "description": "Node wrapper for therun.gg's API.",
  "author": "Devin Rowan",
  "license": "MIT",
  "keywords": [
    "therungg",
    "therun",
    "speedrun",
    "speedrunning"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/developerrowan/therun.git"
  },
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.umd.cjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && vite build",
    "lint": "eslint --fix src && prettier --write src",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:run": "vitest run",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/node": "^18.11.17",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "@vitest/coverage-c8": "^0.26.0",
    "eslint": "^8.30.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.0",
    "prettier": "^2.8.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.2",
    "vite-plugin-dts": "^1.7.1",
    "vitest": "^0.26.0"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "arrowParens": "avoid",
    "tabWidth": 2,
    "trailingComma": "es5"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
